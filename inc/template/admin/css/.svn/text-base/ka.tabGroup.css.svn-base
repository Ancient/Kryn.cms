.kwindow-win-titleGroups {
    display: block;
    position: absolute; 
    top: 26px;
    left: 0px;
    min-height: 28px;
}

.kwindow-win-tabGroup {
    margin-left: 15px;
    float: left;
    overflow: hidden;
    border-right: 1px solid #777;
    border-left: 1px solid #777;
    border-top: 1px solid #555;
    -moz-border-radius-topleft: 5px;
    -moz-border-radius-topright: 5px;
    min-width: 20px;
    height: 28px;
    background-color: #bbb;
    background-image: url(../images/win-actionbar-groupButton-bg.png);
    background-repeat: no-repeat;
    white-space: nowrap;
    position: relative;
}

.kwindow-win-tabGroup object {
    position: absolute;
    left: 0px;
    top: 0px;
    width: 27px;
    height: 21px;
}

.kwindow-win-tabGroupContent {
    position: relative;
}

.kwindow-win-tabWrapper {
    /*padding: 6px 4px 6px 4px;
    padding: 3px 4px 2px 4px;
    line-height: 22px;*/
    border-left: 1px solid #a9a9a9;
    border-top: 1px solid #afaeb1;
    border-bottom: 1px solid #bbb;
    margin-left: -1px;
    line-height: 21px;
    font-size: 10px;
    position: relative;
    top: 12px;
    top: 11px;
/*    width: 26px;*/
    padding: 3px 14px 3px 14px;
    color: #555;
    padding: 16px 15px 2px 14px;
    padding: 15px 7px 2px 7px;
    background-position: center 1px;
    background-repeat: no-repeat;
    cursor: default;
}


.kwindow-win-tabWrapperLast {
    -moz-border-radius-topright: 4px;
    border-right: 1px solid #a9a9a9;
/*    -moz-border-radius-bottomright: 4px;*/
}

.kwindow-win-tabWrapperFirst {
    -moz-border-radius-topleft: 4px;
/*    -moz-border-radius-bottomleft: 4px;*/
}


.kwindow-win-tabWrapper:hover {
    background-color: #b0b0b0;
    border-left: 1px solid #999;
    border-bottom: 1px solid #aaa;
}

.kwindow-win-tabGroup .buttonHover {
    background-color: #999;
}

.kwindow-win-tabGroup .buttonHover,
.kwindow-win-tabWrapper:active {
    background-color: #eee;
    border-left: 1px solid #999;
    border-bottom: 1px solid #eee;
    color: #333;
}
